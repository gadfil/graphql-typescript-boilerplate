{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apollo-cache-inmemory": "^1.3.12",
    "apollo-client": "^2.4.8",
    "apollo-link-error": "^1.1.5",
    "apollo-link-http": "^1.5.9",
    "graphql-tag": "^2.10.1",
    "history": "^4.7.2",
    "react": "^16.7.0",
    "react-apollo": "^2.3.3",
    "react-dom": "^16.7.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.3",
    "whatwg-fetch": "^3.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint-graphql": "eslint -c .eslintrc-graphql --ext ts --ext tsx src/",
    "clear-generated": "find . -name \"__generated__\" -exec rm -rf '{}' +",
    "apollo-gen": "apollo client:codegen --target=typescript --localSchemaFile=../server/src/schema/__generated__/schema.graphql --includes=./src/**/*.{ts,tsx} --tagName gql",
    "watch": "npm run clear-generated && concurrently -k -p \"[{name}]\" -n \"CRA,ApolloGen\" -c \"blue.bold,cyan.bold\" \"npm run start \" \"npm run apollo-gen -- --watch\"",
    "watch-gcg": "npm run gcg -- --watch",
    "gcg": "gql-gen --config codegen.yml"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/history": "^4.7.2",
    "@types/jest": "23.3.12",
    "@types/node": "10.12.18",
    "@types/react": "16.7.20",
    "@types/react-dom": "16.0.11",
    "@types/react-router": "^4.4.3",
    "@types/react-router-dom": "^4.3.1",
    "graphql-code-generator": "^0.15.2",
    "graphql-codegen-add": "^0.15.2",
    "graphql-codegen-fragment-matcher": "^0.15.2",
    "graphql-codegen-typescript-client": "^0.15.2",
    "graphql-codegen-typescript-common": "^0.15.2",
    "graphql-codegen-typescript-react-apollo": "^0.15.2",
    "mkdirp": "^0.5.1",
    "typescript": "3.2.2",
    "typescript-eslint-parser": "^22.0.0"
  }
}
